(function () {
  if (!mstrmojo.plugins.{{ name }}) {
    mstrmojo.plugins.{{ name }} = {}
  }

  // Explain some of the generic MSTR JS infrastructure
  mstrmojo.requiresCls(
    'mstrmojo.CustomVisBase',
    'mstrmojo.models.template.DataInterface',
    'mstrmojo.vi.models.editors.CustomVisEditorModel'
  )

  mstrmojo.plugins.{{name}}.{{name}} = mstrmojo.declare(
    mstrmojo.CustomVisBase,
    null,
    {
      supportNEE: true,

      scriptClass: 'mstrmojo.plugins.{{name}}.{{name}}',
      cssClass: '{{lower name}}',
      errorMessage: 'Either there is not enough data to display the visualization or the visualization configuration is incomplete.',
      errorDetails: 'This visualization requires zero attributes and one or more metrics.',
      externalLibraries: [],
      useRichTooltip: false,
      reuseDOMNode: true,
      plot: function () {
        // Code goes here
      }
    }
  )
})()
